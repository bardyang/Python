#二八十六制整数
#bin() 二进制 oct() 八进制 int() 十进制 hex() 十六进制 format() 格式化数字

>>>x = 1234
>>>bin(x)
'0b10011010010'
>>>oct(x)
'0o2322'
>>>hex(x)
'0x4d2'

>>>format(x,'b')
'10011010010'
>>>format(x,'o')
'2322'
>>>format(x,'x')
'4d2'

>>>x = -1234
>>>format(x,'b')
'-10011010010'
>>>format(x,'x')
'-4d2'

#转换成十进制
>>>int('4d2',16)
1234
>>>int('10011010010',2)
1234
